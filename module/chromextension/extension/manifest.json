{
  "name": "Nystem server",
  "version": "0.1",
  "description": "Nystem server",
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "128": "icon/128.png",
    "512": "icon/512.png"
