{
  "name": "Youtube queue",
  "version": "0.1",
  "description": "Youtube queue",
  "icons": {
    "16": "icons/youtube.png",
    "32": "icons/youtube.png",
    "48": "icons/youtube.png",
    "128": "icons/youtube.png"
  },
  "browser_action": {
    "default_icon": "icons/youtube.png",
    "default_title": "Youtube queue",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": ["tabs", "contextMenus", "webNavigation"]
}
